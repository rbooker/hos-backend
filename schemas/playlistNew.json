    "$id": "http://our.company.com/companyUpdate.schema.json",
    "type": "object",
    "properties": {
      "date":{
          "type": "string",
          "format": "date"
      },
      "description": {
        "type": "string"
      },
      "memberID":{
        "type": "integer",
