{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.company.com/companyUpdate.schema.json",
    "type": "object",
    "properties": {
      "username": {
        "type": "string"
      },
      "memberID": {
        "type": "integer",
        "minimum": 1
      },
      "showID": {
        "type": "integer",
        "minimum": 1
      }
      
    },
    "additionalProperties": false,
    "required": ["username", "memberID", "showID"]
  }